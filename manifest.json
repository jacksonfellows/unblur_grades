{
	"manifest_version": 2,
	"name": "Unblur Grades",
	"version": "0.1",
	"description": "Allows you to unblur your grades in Canvas",
	"author": "Jackson Fellows",

	"icons": {
		"48": "icons/icon.png",
		"96": "icons/icon@2x.png"
	},

	"content_scripts": [
		{
			"matches": ["*://*.instructure.com/*/grades"],
			"js": ["unblur.js"]
		}
	]
}
